    "from matplotlib.pyplot import plot, show, xlabel, ylabel, title\n",
    "from numpy import linspace\n",
    "r=float(input(\"Introduce el valor de r: \"))\n",
    "x=0.5 #valor inicial de x\n",
    "for n in range(1000): #Hago las mil primeras iteraciones \n",
    "    xprima=r*x*(1-x)\n",
    "    x=xprima #Cambio el valor de x por el de xprima\n",
    "for n in range(1000): #Hago las siguientes mil iteraciones\n",
    "    plot(r,x,\"ko\") #Voy representando los valores que toma x frente al valor de r escogido\n",
    "    xprima=r*x*(1-x)\n",
    "    x=xprima\n",
    "xlabel(\"Valores de r\")\n",
    "ylabel(\"Valores de x\")\n",
    "title(\"Representación de r frente a x\")\n",
    "show()\n",
    "#Ejercicio1.2\n",
    "r=linspace(1,4,300) #En este caso, r toma distintos valores entre 1 y 4 en pasos de 0.01\n",
    "x=0.5 #tomamos un valor de x inicial\n",
    "#Hacemos el mismo proceso que antes pero variando r\n",
    "for n in range(1000): #primeras mil iteraciones\n",
    "    xprima=r*x*(1-x) #xprima es un vector cuyos elementos serán los valores de la iteración para distintos r\n",
    "    x=xprima #Cambio el valor de x por el xprima calculado antes\n",
    "for n in range(1000): #segundas mil iteraciones \n",
    "    plot(r,x,\"k,\") #vamos representando los valores que toma x frente a r\n",
    "    xprima=r*x*(1-x)\n",
    "    x=xprima      \n",
    "xlabel(\"Valores de r\")\n",
    "ylabel(\"Valores de x\")\n",
    "title(\"Gráfica de Feigenbaum\")\n",
    "show()"
