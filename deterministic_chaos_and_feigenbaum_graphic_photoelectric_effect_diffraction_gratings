   "source": [
    "#Ejercicio 2.1\n",
    "from matplotlib.pyplot import plot, show, xlabel, ylabel, title\n",
    "from numpy import loadtxt\n",
    "from numpy import zeros\n",
    "datos=loadtxt(\"millikan.txt\",float) #Importo los datos del fichero millikan.txt\n",
    "x=datos[:,0] #Guardo la primera columna en x\n",
    "y=datos[:,1] #Guardo la segunda columna en y\n",
    "plot(x,y, \"ro\") #Represento el potencial frente a la frecuencia \n",
    "xlabel(\"Frecuencia (Hz)\")\n",
    "ylabel(\"Potencial (V)\")\n",
    "title(\"Datos de Millikan\")\n",
    "show()\n",
    "#Ejercicio 2.2\n",
    "N=len(x) #N es el número de datos de cada vector x ó y  \n",
    "Ex=sum(x)/N #Sumo todos los elementos de x y lo divido entre el número de datos\n",
    "Ey=sum(y)/N\n",
    "S=[] #Defino una lista vacía \n",
    "J=[] \n",
    "for i in range(N): #Hago el bucle N veces\n",
    "    s=x[i]*x[i] #Elevo el elemento xi al cuadrado \n",
    "    j=x[i]*y[i]\n",
    "    S.append(s) #Añado a la lista el elemento s que he calculado antes \n",
    "    J.append(j)\n",
    "Exx=sum(S)/N\n",
    "Exy=sum(J)/N\n",
    "m=(Exy-Ex*Ey)/(Exx-Ex*Ex) #Calculo la pendiente\n",
    "c=(Exx*Ey-Ex*Exy)/(Exx-Ex*Ex) #Calculo la ordenada en el origen \n",
    "Puntosy=zeros(N) #Construyo un vector fila de ceros de N valores \n",
    "for i in range(N):\n",
    "    Puntosy[i]=m*x[i]+c #Usando los valores de m y c de antes, calculo los Puntosy para cada elemento de x\n",
    "plot(x,Puntosy) #Represento los valores de x y los obtenidos de Puntosy\n",
    "plot(x,y,\"ko\")\n",
    "xlabel(\"Frecuencia (Hz)\")\n",
    "ylabel(\"Potencial (V)\")\n",
    "title(\"Datos de Millikan con ajuste de mínimos cuadrados\")\n",
    "show()\n",
    "h=m*1.602*10**(-19) #saco h a partir de la pendiente\n",
    "print(\"El valor de la pendiente es m=\",m, \"V/Hz\")\n",
    "print(\"El valor de la ordenada en el origen es c=\",c, \"V\")\n",
    "print(\"El valor de la constante de Planck es \", h, \"J·s\")"
   ]
  },
